- extends 'subterreader/base.html.haml'

- block content
  - include 'subterreader/add.html.haml'
  - if documents_list
    %ul
      - for document in documents_list
        %li 
          %a{'href': "{{ document.url }}/"}= document.url
    %br
    %p
      %a{'href': "read"} Read
  - else
    %p No documents are available.
